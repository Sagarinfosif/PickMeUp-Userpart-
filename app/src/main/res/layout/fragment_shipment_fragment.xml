<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.Deliver.Fragment.ShipmentFragment">


    <include
        android:id="@+id/include"
        layout="@layout/top_bar_deliver" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/include"
        app:layout_constraintBottom_toBottomOf="parent"
        >


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                >


                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/imgTruck"
                    android:layout_width="@dimen/space150"
                    android:layout_height="@dimen/space150"
                    android:src="@drawable/dsfdsfsfd"
                    android:layout_gravity="center_horizontal"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/cardNew"
                    android:layout_marginLeft="@dimen/space30"
                    android:layout_marginTop="@dimen/space20"
                    android:layout_marginRight="@dimen/space30"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/imgTruck">


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/black">


                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/imgNew"
                            android:layout_width="@dimen/space60"
                            android:layout_height="@dimen/space60"
                            android:layout_marginTop="@dimen/space20"
                            android:src="@drawable/___1_"
                            android:tint="@color/white"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                        <androidx.appcompat.widget.AppCompatTextView
                            style="@style/textWhitGray16sp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/space10"
                            android:layout_marginBottom="@dimen/space20"
                            android:text="@string/new_shipment"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/imgNew" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>



                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="@dimen/space5"
                    android:layout_marginLeft="@dimen/space30"
                    android:id="@+id/cardCurrentShipment"
                    android:layout_marginTop="@dimen/space20"
                    android:layout_marginRight="@dimen/space30"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/imgTruck">


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        >


                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/imgCurrent"
                            android:layout_width="@dimen/space60"
                            android:layout_height="@dimen/space60"
                            android:layout_marginTop="@dimen/space20"
                            android:src="@drawable/sasad"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                        <androidx.appcompat.widget.AppCompatTextView
                            style="@style/textBalck16sp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/space10"
                            android:layout_marginBottom="@dimen/space20"
                            android:text="@string/current_shipment"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/imgCurrent" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>



                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="@dimen/space5"
                    android:layout_marginLeft="@dimen/space30"
                    android:layout_marginTop="@dimen/space20"
                    android:layout_marginRight="@dimen/space30"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/imgTruck">


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        >


                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/imgInvoice"
                            android:layout_width="@dimen/space60"
                            android:layout_height="@dimen/space60"
                            android:layout_marginTop="@dimen/space20"
                            android:src="@drawable/ic_bill"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                        <androidx.appcompat.widget.AppCompatTextView
                            style="@style/textBalck16sp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/space10"
                            android:layout_marginBottom="@dimen/space20"
                            android:text="My Invoice"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/imgInvoice" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="@dimen/space5"
                    android:layout_marginLeft="@dimen/space30"
                    android:layout_marginTop="@dimen/space20"
                    android:layout_marginRight="@dimen/space30"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/imgTruck"
                    android:layout_marginBottom="@dimen/space30"
                    >


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"

                        >


                        <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/imgFreight"
                            android:layout_width="@dimen/space60"
                            android:layout_height="@dimen/space60"
                            android:layout_marginTop="@dimen/space20"
                            android:src="@drawable/ic_bill"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                        <androidx.appcompat.widget.AppCompatTextView
                            style="@style/textBalck16sp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/space10"
                            android:layout_marginBottom="@dimen/space20"
                            android:text="My Invoice"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/imgFreight" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>


            </LinearLayout>




        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.core.widget.NestedScrollView>




</androidx.constraintlayout.widget.ConstraintLayout>